{
	"flask": {
		"host": "0.0.0.0",
		"port": 8080,
		"debug": false,
		"threaded": true,
		"processes": 1
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
 		"pyLibrary.env.big_data.MAX_STRING_SIZE": 100000000
	},
	"request_logs": {
		"host": "http://localhost",
		"port": 9400,
		"index": "active_data_requests",
		"type": "request_log",
		"schema": {
			"$ref": "//../schema/request_log.schema.json"
		}
	},
	"saved_queries":{
		"host": "http://localhost",
		"port": 9400,
		"index": "saved_queries",
		"type": "query",
		"debug": true
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9400,
		"index": "testdata",
		"type": "test_result",
		"debug": false
	},
	"debug": {
		"trace": true,
		"cprofile": false,
		"log": [
			{
				"log_type": "console"
			}
